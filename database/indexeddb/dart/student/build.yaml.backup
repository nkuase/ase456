targets:
  $default:
    builders:
      build_web_compilers:entrypoint:
        generate_for:
          - web/**.dart
        options:
          compiler: dart2js
          dart2js_args:
            - --fast-startup
            - --minify
            - --trust-type-annotations

  lib_apps:
    builders:
      build_web_compilers:entrypoint:
        generate_for:
          - lib/app1.dart
          - lib/app2.dart
        options:
          compiler: dart2js
          dart2js_args:
            - --fast-startup
            - --minify
            - --trust-type-annotations